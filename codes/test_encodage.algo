ALGORITHME LesConversions
variable:
   e             : entier 
   r             : réel
   c             : caractère
   ch            : chaine de caractères
DEBUT
   c    <- 'a'
   e    <- ord ( c )
   
   e    <- e - 32
   c    <- car ( e )
   
   ecrire ( e )
   
   c <- car ( ord ( 'b' ) - 32 )
   
   ecrire ( c )
   
   e <- enEntier ( "12"    )
   r <- enReel   ( "12.32" )
   
   ch <- enChaine ( 12.5 )
   
   ecrire ( e  )
   ecrire ( r  )
   ecrire ( ch )
   
FIN	